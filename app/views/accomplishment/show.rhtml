<% for column in Accomplishment.content_columns %>
<p>
  <b><%= column.human_name %>:</b> <%=h @accomplishment.send(column.name) %>
</p>
<% end %>

<%= link_to 'Edit', :action => 'edit', :id => @accomplishment %> |
<% if session[:last_index] %>
<%= link_to 'Back', session[:last_index] %>
<% else %>
<%= link_to 'Back', :controller => 'waterlogged', :action => 'index' %>
<% end %>
